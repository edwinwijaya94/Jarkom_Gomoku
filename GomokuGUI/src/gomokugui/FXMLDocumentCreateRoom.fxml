<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="224.0" prefWidth="321.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gomokugui.FXMLDocumentCreateRoomController">
  <children>
      <VBox alignment="TOP_CENTER" prefHeight="183.0" prefWidth="321.0" spacing="35.0">
         <children>
            <Label text="Create Room">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Label text="Room Name : " />
                  <TextField fx:id="newRoomName" prefHeight="25.0" prefWidth="212.0">
                     <HBox.margin>
                        <Insets right="12.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
         </children>
      </VBox>
    <HBox id="HBox" alignment="CENTER" minWidth="-1.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
      <children>
        <Button fx:id="okButton" mnemonicParsing="false" onMouseClicked="#clickOk" prefWidth="80.0" text="OK" />
        <Button fx:id="cancelButton" mnemonicParsing="false" onMouseClicked="#clickCancel" prefWidth="80.0" text="Cancel" />
      </children>
    </HBox>
  </children>
</AnchorPane>
